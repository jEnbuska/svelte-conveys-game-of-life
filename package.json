		"@sveltejs/kit": "next",
		"@testing-library/jest-dom": "^5.14.0",
		"@testing-library/svelte": "^3.0.0",
		"@types/cookie": "^0.5.1",
		"@types/jest": "^27.0.0",
		"@types/node": "^18.7.14",
		"@types/testing-library__jest-dom": "^5.14.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"babel-jest": "^27.0.0",
		"cross-env": "^7.0.3",
		"eslint": "8.22.0",
