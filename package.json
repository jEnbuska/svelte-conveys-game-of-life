		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-jester": "^2.0.1",
		"svelte-preprocess": "^4.10.6",
		"ts-jest": "^27.0.0",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0-beta.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"cookie": "^0.4.1"
	}
